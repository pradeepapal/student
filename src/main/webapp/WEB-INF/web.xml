<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
 
  
  <servlet>
  <servlet-name>create</servlet-name>
  <servlet-class>com.ty.controller.CreateStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>create</servlet-name>
  <url-pattern>/create</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>delete</servlet-name>
  <servlet-class>com.ty.controller.DeleteStudentDao</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>delete</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>edit</servlet-name>
  <servlet-class>com.ty.controller.EditStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>edit</servlet-name>
  <url-pattern>/edit</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>display</servlet-name>
  <servlet-class>com.ty.controller.DisplayStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>display</servlet-name>
  <url-pattern>/display</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>getStu</servlet-name>
  <servlet-class>com.ty.controller.GetStudent</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>getStu</servlet-name>
  <url-pattern>/getstudent</url-pattern>
  </servlet-mapping>
  
  
  	<servlet>
  		<servlet-name>Login</servlet-name>
  		<servlet-class>com.ty.controller.LoginServlet</servlet-class>
  	</servlet>
  	<servlet-mapping>
  		<servlet-name>Login</servlet-name>
  		<url-pattern>/login</url-pattern>
  	</servlet-mapping>
  	<servlet>
  		<servlet-name>Logout</servlet-name>
  		<servlet-class>com.ty.controller.LogoutServlet</servlet-class>
  	</servlet>
  	<servlet-mapping>
  		<servlet-name>Logout</servlet-name>
  		<url-pattern>/logout</url-pattern>
  	</servlet-mapping>
  
  
</web-app>
